<h1>List of Pre-historic Creatures</h1>
<form method='GET' action='/prehistoric_creatures'>
    <label for='typeFilter'>Filter by Type</label>
    <input type='text' id='typeFilter' name='typeFilter'>
    <input type='submit'>
</form>
<ul id = "creatureIndex">
    <% creatures.forEach(creature => { %>
        <li class = "creature"><%= creature.type %></li>
        <img src=<%= creature.img_url %>>
    <% }) %>
</ul>