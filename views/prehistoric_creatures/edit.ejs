<h1>Creature Edit Form</h1>

<form method="POST" action="/prehistoric_creatures/<%= creatureId %>/?_method=PUT">
    <label for='creatureType'>Type</label>
    <input type='text' name='type' id='creatureType' value="<%=creature.type%>">

    <label for='img_url'>Image URL</label>
    <input type='text' name='img_url' id='img_url' value = <%= creature.img_url %>>

    <input type='submit'>
</form>